              <xs:element name="Password" type="xs:string" msdata:Ordinal="6" />
              <xs:element name="EmailId" type="xs:string" msdata:Ordinal="7" />
              <xs:element name="Active" type="xs:string" msdata:Ordinal="8" />
            </xs:sequence>
            <xs:attribute name="ChannelId" type="xs:int" use="required" />
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
    <xs:element name="ChannelDetails" type="Channel_Info" />
